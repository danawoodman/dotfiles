{
  "Arrow function": {
    "prefix": "fu",
    "body": ["(${1:args}) => {", "  $0", "}"]
  },
  "Annonymous function": {
    "prefix": "fun",
    "body": ["function (${1:args}) {", "  $0", "}"]
  },
  "describe test block": {
    "prefix": "desc",
    "body": ["describe('${1:some scenario}', () => {", "  $0", "})"]
  },
  "before block": {
    "prefix": "before",
    "body": ["before(async () => {", "  $0", "})"]
  },
  "before each block": {
    "prefix": "be",
    "body": ["beforeEach(async () => {", "  $0", "})"]
  },
  "test block": {
    "prefix": "test",
    "body": ["test('${1:should do stuff}', async () => {", "  $0", "})"]
  },
  "pending test block": {
    "prefix": "xtest",
    "body": [
      "test.skip('${1:should do stuff}', async () => {",
      "  expect(true).toBe(false)$0",
      "})"
    ]
  },
  "test expect": {
    "prefix": "exp",
    "body": ["expect(${1:foo}).toBe($0)"]
  },
  "Named function": {
    "prefix": "func",
    "body": ["function ${1:name}(${2:args}): ${3:void} {", "  $0", "}"]
  },
  "Named React component function": {
    "prefix": "funcc",
    "body": [
      "function ${1:name}(${2:args}): JSX.Element {",
      "  return ($0)",
      "}"
    ]
  },
  "ES6 Class": {
    "prefix": "cla",
    "body": ["class ${1:Name} {", "  $0", "}"]
  },
  "Module exports": {
    "prefix": "me",
    "body": ["module.exports = $0"]
  },
  "ES6 default export": {
    "prefix": "ed",
    "body": ["export default $0"]
  },
  "Console log": {
    "prefix": "log",
    "body": ["console.log($0)"],
    "description": "Console log"
  },
  "CommonJS require": {
    "prefix": "req",
    "body": ["const $1 = require('$0')"],
    "description": "CommonJS require"
  },
  "Default Import": {
    "prefix": "im",
    "body": ["import $1 from '$0'"],
    "description": "Default import statement"
  },
  "ES6 Import": {
    "prefix": "imp",
    "body": ["import { $1 } from '$0'"],
    "description": "ES6 import statement"
  },
  "React component file": {
    "prefix": "comp",
    "body": [
      "import React from 'react'",
      "",
      "export interface $1Props {}",
      "",
      "export default function ${1:name}(${2:args}): JSX.Element {",
      "  return ($0)",
      "}"
    ]
  },
  "Import classnames lib": {
    "prefix": "cn",
    "body": [
      "import classnames from \"classnames\"",
      "const classes = classnames($0)"
    ]
  },
  "exported interface": {
    "prefix": "ei",
    "body": ["export interface ${1:MyInterface} {", "  $0", "}"]
  },
  "useState": {
    "prefix": "us",
    "body": [
      "const [${1:loading}, set${2:Loading}] = React.useState<${3:boolean}>(${4:false})"
    ]
  },
  "useRef": {
    "prefix": "ur",
    "body": ["const ${1:ref} = React.useRef<${2:HTMLDivElement}>(${3:null})"]
  }
}
