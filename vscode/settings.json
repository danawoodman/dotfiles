{
  "window.zoomLevel": 0,
  //---------------------------------------------------------------------------
  // Editor
  //---------------------------------------------------------------------------
  "editor.fontFamily": "'Fira Code', Inconsolata, 'Droid Sans Mono', Consolas, 'Liberation Mono', Menlo, Monaco, 'Source Code Pro', 'Annonymouse Pro', 'Andale Mono', 'Space Mono', 'M+ 1m'",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.tabSize": 2,
  "editor.letterSpacing": 1.3,
  "editor.lineHeight": 30,
  "editor.snippetSuggestions": "top",
  "editor.folding": true,
  "editor.tabCompletion": true,
  "editor.wordBasedSuggestions": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.minimap.enabled": false,
  "editor.foldingStrategy": "indentation",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
    // "source.fixAll": true,
    //   "source.organizeImports": true,
  },
  "vscode_custom_css.imports": [
    "file:///Users/danawoodman/code/dotfiles/synthwave86.css",
    "file:///Users/danawoodman/code/dotfiles/vscode.css"
  ],
  "vscode_custom_css.policy": true,
  //---------------------------------------------------------------------------
  // File handling
  //---------------------------------------------------------------------------
  "files.associations": {
    "*.jsp": "jsp",
    "*.css": "postcss",
    "*.nc": "gcode"
  },
  //---------------------------------------------------------------------------
  // Workbench
  //---------------------------------------------------------------------------
  "workbench.colorCustomizations": {
    "editorError.border": "#ff3300",
    "editorError.foreground": "#ff0000"
  },
  "workbench.startupEditor": "newUntitledFile",
  //---------------------------------------------------------------------------
  // Explorer
  //---------------------------------------------------------------------------
  "explorer.openEditors.visible": 0,
  "explorer.confirmDragAndDrop": false,
  "tslint.autoFixOnSave": true,
  // "git.autofetch": false,
  "files.trimTrailingWhitespace": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[nim]": {
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    // "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    // "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    // "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    // "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[htmltag]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "prettier.disableLanguages": ["vue"],
  "tailwindCSS.includeLanguages": {
    "typescript": "javascript",
    "typescriptreact": "javascript"
  },
  //---------------------------------------------------------------------------
  // Shell
  //---------------------------------------------------------------------------
  "shellformat.path": "/usr/local/bin/shfmt",
  "typescript.tsserver.log": "verbose",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
    // "svelte"
  ],
  // "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.enable": true,
  // "eslint.autoFixOnSave": false,
  //---------------------------------------------------------------------------
  // Golang
  //---------------------------------------------------------------------------
  "go.lintTool": "golangci-lint",
  "go.lintFlags": ["--fast"],
  // "go.buildTags": "-tags 'linux darwin windows'",
  "go.languageServerExperimentalFeatures": {
    "format": true,
    "autoComplete": true,
    "rename": true,
    "goToDefinition": true,
    "hover": true,
    "signatureHelp": true,
    "goToTypeDefinition": true,
    "goToImplementation": true,
    "documentSymbols": true,
    "workspaceSymbols": true,
    "findReferences": true,
    "diagnostics": true,
    "documentLink": true
  },
  "go.useLanguageServer": true,
  //---------------------------------------------------------------------------
  // Arduino/C/C++
  //---------------------------------------------------------------------------
  // "C_Cpp.default.includePath": [
  //   "/Users/danawoodman/Documents/Arduino/libraries/**"
  // ],
  //---------------------------------------------------------------------------
  // HTML
  //---------------------------------------------------------------------------
  "html.format.enable": true,
  "html.format.wrapLineLength": 80,
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.quickSuggestions": false
  },
  //---------------------------------------------------------------------------
  // Emmet
  //---------------------------------------------------------------------------
  "emmet.includeLanguages": {
    "javascript": "html",
    "jsp": "html",
    "postcss": "css",
    "jsont": "html"
  },
  "emmet.syntaxProfiles": {
    "postcss": "css",
    "jsont": "html"
  },
  // "emmet.useNewEmmet": true,
  "emmet.showExpandedAbbreviation": "always",
  //---------------------------------------------------------------------------
  // Vim
  //---------------------------------------------------------------------------
  // "vim.debug.loggingLevel": "debug",
  // "vim.disableAnnoyingNeovimMessage": true,
  "vim.foldfix": true,
  "vim.hlsearch": true,
  "vim.leader": ",",
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<leader>", "<space>"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.commentLine",
          "args": []
        }
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "m"],
      "after": [],
      "commands": [
        {
          "command": "dakara-foldplus.cursor.exclude",
          "args": []
        }
      ]
    },
    {
      "before": ["<space>"],
      "after": [],
      "commands": [
        {
          "command": "editor.foldRecursively",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "<space>"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.commentLine",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "f"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.quickOpen",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "w"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.files.save",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "q"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.files.save",
          "args": []
        },
        {
          "command": "workbench.action.closeActiveEditor",
          "args": []
        }
      ]
    }
  ],
  "terminal.integrated.fontSize": 17,
  // "github.hosts": [
  //   {
  //     "host": "https://github.com",
  //     "username": "oauth",
  //     "token": "system"
  //   }
  // ],
  // "arduino.path": "/Applications",
  // "shellformat.runOnSave": true,
  "git.autofetch": true,
  "[gcode]": {},
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust"
  },
  "todo-tree.highlights.enabled": true,
  "javascript.preferences.importModuleSpecifier": "relative",
  "files.exclude": {
    "node_modules": true
  },
  "typescript.tsserver.maxTsServerMemory": 8192,
  "todo-tree.tree.showScanModeButton": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "dart.flutterSdkPath": "/Users/danawoodman/flutter",
  "dart.openDevTools": "flutter",
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "workbench.colorTheme": "One Dark Pro",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.formatOnSave": true,
  "vsicons.dontShowNewVersionMessage": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.enablePreview": true,
  "jest.restartJestOnSnapshotUpdate": true,
  "jest.showCoverageOnLoad": true,
  "rust.mode": "rls",
  "rust-client.channel": "stable",
  "rust.rustup": {
    "toolchain": "stable-x86_64-apple-darwin"
  },
  "rust-client.rustupPath": "$HOME/.cargo/bin/rustup",
  "search.exclude": {
    "package-lock.json": true,
    "yarn.lock": true
  }
}
